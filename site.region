<!doctype html>
<html>
<!--  Squarespace theme by Dylan Hanson-->
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

    <!-- include system scripts, page meta, and header code injection -->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div class="site-container">

      <header class="site-header noselect nodrag nocursor">

        <!-- site title or logo -->
        {.section website}
        <div class="site-branding rel-center site-width">
          <div class="site-title text-center">
            <a href="/">
              That's What She Said
            </a>
          </div>
          <div class="site-subtitle text-center">
            <table class="rel-center">
              <tr>
                <td class="info date text-left">
                  <span></span>
                </td>
                <td class="text-center">
                  <span>BB&#38;N&#146;s Intersectional Feminist Newsletter</span>
                </td>
                <td class="info volume text-right">
                  <span></span>
                </td>
              </tr>
            </table>
          </div>
        </div>
        {.end}

        <!-- site navigation -->
        <!-- <squarespace:navigation navigationId="siteNav" template="site-navigation" /> -->

      </header>

      <!-- cms content injection point -->
      <div class="image-spread-container site-width rel-center noselect nocursor nodrag">
        <div class="image-spread nodrag noselect nocursor">
<!--      Injection point for image background-->
        </div>
        <div class="image-description nodrag noselect nocursor">
          <div class="text-right">
            <h3>
              <span></span>
            </h3>
          </div>
        </div>
      </div>

      <div class="site-width rel-center">
        <div class="content-width rel-center">

          <main class="content-container" role="main" data-content-field="main-content">
            {squarespace.main-content}
          </main>

        </div>
      </div>

    </div>

    <footer class="site-footer"></footer>

    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

    <!-- other scripts, and footer code injection -->
    {squarespace-footers}

  </body>
</html>
